{
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "extends": [
        "airbnb/base"
    ],
    "rules": {
        "semi": "off",
        "max-len": [
            "error",
            150,
            {
                "ignoreUrls": true
            }
        ]
    }
}